{
    "name": "nono",
    "version": "1.0.0",
    "description": "A retro-styled nonogram puzzle game built with Phaser 3, controlled via gamepad",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "format": "prettier --write \"public/js/**/*.js\" \"server.js\""
    },
    "dependencies": {
        "express": "^4.18.2",
        "phaser": "^3.70.0"
    },
    "devDependencies": {
        "prettier": "^3.1.0"
    },
    "prettier": {
        "printWidth": 130,
        "tabWidth": 4,
        "trailingComma": "none",
        "semi": false
    },
    "keywords": [
        "nonogram",
        "puzzle",
        "phaser",
        "gamepad",
        "retro"
    ],
    "author": "",
    "license": "MIT"
}
